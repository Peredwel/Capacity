{
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "env": {
    "browser": true,
    "node": false
  },
  "rules": {
    "camelcase": 0,      
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "no-unused-vars": 1,
    "no-undef": 1,
    "no-param-reassign": 1,
    "space-infix-ops": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "prefer-template": 0
  }
}