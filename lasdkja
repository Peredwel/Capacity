       \r\n      });\r\n    },\r\n    checkOut ()\r\n    {\r\n      api.checkIn(this, this.place_id, response => {\r\n        this.checked_in = (this.place_id == api.checked_in_place);\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    // react to route changes...\r\n    // When the user goes to another page\r\n    '$route' (to, from) {\r\n      console.log('asdf');\r\n      this.updatePlace();\r\n    }\r\n  }\r\n}\r\n</script>\n\n\n// WEBPACK FOOTER //\n// place.vue?b69071ac","var Component = require(\"!../../../node_modules/vue-loader/lib/component-normalizer\")(\n  /* script */\n  require(\"!!babel-loader!../../../node_modules/vue-loader/lib/selector?type=script&index=0!./nav_about.vue\"),\n  /* template */\n  require(\"!!../../../node_modules/vue-loader/lib/template-compiler?{\\\"id\\\":\\\"data-v-7a844b88\\\"}!../../../node_modules/vue-loader/lib/selector?type=template&index=0!./nav_about.vue\"),\n  /* scopeId */\n  null,\n  /* cssModules */\n  null\n)\n\nmodule.exports = Component.exports\n\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./src/components/navmodals/nav_about.vue\n// module id = 28\n// module chunks = 0","var Component = require(\"!../../../node_modules/vue-loader/lib/component-normalizer\")(\n  /* script */\n  require(\"!!babel-loader!../../../node_modules/vue-loader/lib/selector?type=script&index=0!./nav_account.vue\"),\n  /* template */\n  require(\"!!../../../node_modules/vue-loader/lib/template-compiler?{\\\"id\\\":\\\"data-v-3e5da71c\\\"}!../../../node_modules/vue-loader/lib/selector?type=template&index=0!./nav_account.vue\"),\n  /* scopeId */\n  null,\n  /* cssModules */\n  null\n)\n\nmodule.exports = Component.exports\n\n\n\n//////////////////\n// WEBPACK FOOTER\n// ./src/components/navmodals/nav_account.vue\n// module id = 29\n// module chunks = 0","\n/* styles */\nrequire(\"!!../../../node_modules/extract-text-webpack-plugin/loader.js?{\\\"omit\\\":1,\\\"remove\\\":true}!vue-style-loader!css-loader?{\\\"minimize\\\":true,\\\"sourceMap\